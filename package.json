{
  "name": "rezept",
  "version": "1.0.0",
  "description": "Beispiel",
  "main": "dist/server.js",
  "engines": {
    "node": ">=14.15.5",
    "npm": ">=6.14.11"
  },
  "type": "commonjs",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "npm run init-dist && nodemon --exec ts-node --files src/server.ts --unhandled-rejections=strict",
    "start:debug": "npm run prepare && nodemon --inspect dist/server.js ",
    "pretest": "npm run init-dist",
    "test": "jest --detectOpenHandles --forceExit",
    "test:coverage": "npm t -- --coverage",
    "appmetrics": "echo Umgebungsvariable setzen:   NODE_OPTIONS=\"--require appmetrics-dash/attach\"",
    "asciidoc": "ts-node scripts/asciidoc",
    "docker-build": "ts-node scripts/docker-build",
    "clinic": "clinic doctor -- node dist/server.js",
    "clean": "ts-node scripts/clean",
    "curl:heroku": "curl --include --tlsv1.2 --insecure https://fathomless-caverns-91920.herokuapp.com/rezepte/00000000-0000-0000-0000-000000000001",
    "curl:openshift": "curl --include http://beispiel-jz.apps.us-east-1.starter.openshift-online.com:3000/rezepte/00000000-0000-0000-0000-000000000001",
    "depcheck": "depcheck",
    "deploy:heroku": "git push heroku master",
    "deploy:openshift": "npm run deploy:prepare && npm exec nodeshift --expose --deploy.port 3000",
    "deploy:prepare": "concurrently --prefix \"[{name}]\" --names \"ESLint,ejs-lint,init-dist,TypeScript\" --prefix-colors \"yellow.bold,gray.bold,cyan.bold,green.bold\" \"npm run eslint\" \"npm run ejs-lint\" \"npm run init-dist\" \"npm run tsc\"",
    "ejs-lint": "node ./node_modules/ejs-lint/index.js",
    "eslint": "eslint src/**/*.ts",
    "init-dist": "ts-node scripts/init-dist",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "open:heroku": "heroku open --app fathomless-caverns-91920 /api/rezepte/00000000-0000-0000-0000-000000000001",
    "open:heroku:file": "heroku open --app fathomless-caverns-91920 /api/rezepte/00000000-0000-0000-0000-000000000001/file",
    "open:heroku:html": "heroku open --app fathomless-caverns-91920 /html/suche",
    "open:heroku:playground": "heroku open --app fathomless-caverns-91920 /api",
    "port-forward": "ts-node scripts/port-forward",
    "prettier": "prettier --write src/**/*.ts scripts/*.?s test/**/*.ts",
    "revealjs": "asciidoctor-revealjs doc/folien/folien.adoc",
    "snyk:auth": "echo https://app.snyk.io/account && snyk auth",
    "snyk": "snyk test",
    "typedoc": "npx typedoc src",
    "tsc": "npx tsc",
    "heroku-postbuild": "npm run tsc --version && npm run tsc && node scripts/init-heroku"
  },
  "files": [
    "dist",
    "package.json",
    ".env",
    ".gitignore",
    ".npmrc",
    "tsconfig.json",
    "scripts"
  ],
  "private": true,
  "homepage": "https://www.hs-karlsruhe.de",
  "keywords": [
    "Node",
    "npm",
    "Express",
    "body-parser",
    "validator",
    "TypeScript",
    "ESLint",
    "REST",
    "GraphQL",
    "Apollo",
    "MongoDB",
    "mongoose",
    "JWT",
